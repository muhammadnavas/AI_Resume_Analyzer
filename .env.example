# Environment Variables for AI Resume Analyzer

# Google Gemini API Configuration
REACT_APP_GEMINI_API_KEY=your_gemini_api_key_here

# Application Configuration
REACT_APP_VERSION=1.0.0
REACT_APP_NAME=AI Resume Analyzer

# Features Configuration
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_VECTOR_SEARCH=false
REACT_APP_MAX_FILE_SIZE=10485760

# Development Configuration
GENERATE_SOURCEMAP=false
DISABLE_ESLINT_PLUGIN=false

# Build Configuration
PUBLIC_URL=/

# Hackathon Information
REACT_APP_HACKATHON_NAME=GenAI-Powered Solutions: Hack The Matrix
REACT_APP_HACKATHON_DATE=September 21, 2025